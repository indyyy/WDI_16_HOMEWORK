<% @alpaca.errors.full_messages.each do |message| %>
<p> <%= message %> </p>
<% end %>

<form action="/alpacas" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type ="text" name="url" value="<%= @alpaca.url %>">
  <button>create</button>
</form>